{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "curly": [0],
    "no-underscore-dangle": [0],
    "prefer-const": [2]
  }
}
